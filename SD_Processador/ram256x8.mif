-- Quartus Prime generated Memory Initialization File (.mif)

WIDTH=8;
DEPTH=256;

ADDRESS_RADIX=UNS;
DATA_RADIX=BINARY;

CONTENT BEGIN
    0 : 10010000;  -- IN A
    1 : 10010001;  -- IN B
    2 : 00000010;  -- ADD A, B
    3 : 10100011;  -- OUT R
    4 : 11110000;  -- WAIT
    [5..255] : 00000000;  -- Preenche os endere√ßos restantes com 0 (NOP)
END;
